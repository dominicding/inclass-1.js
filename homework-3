function setup(){
  createCanvas(400,400);
  background(255);
  noStroke();

  for(a=20; a<480; a=a+20){
      for(var b=40; b<480; b=b+20){
         var c=random(255);
         var d=random(255);
         var e=random(255);
         fill(c,d,e);
         ellipseMode(CENTER);
         ellipse(a,b,20,20);
      }
  }
}
