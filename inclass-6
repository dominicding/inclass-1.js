function setup() {
  createCanvas(200,200);
  background(255);
}
var Circles = 0;
noLoop();

function draw() {
  var a=random(255);
  var b=random(255);
  var c=random(255);
  stroke(a,b,c);
  fill(a,b,c);
  var d=random(25);
  var e=random(150);
  var f=random(150);
  ellipseMode(CENTER);
  ellipse(e,f,d,d);
  Circles = Circles + 1;
  if (Circles >200){
    noLoop();
  }

}
